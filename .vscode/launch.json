  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) teos get info",
      "type": "cppdbg",
      "request": "launch",
      "program": "/mnt/hgfs/Workspaces/EOS/Logos/teos/build/teos",
      "args": ["tokenika", "get", "info", "-h"],
      "stopAtEntry": false,
      "cwd": "/mnt/hgfs/Workspaces/EOS/Logos/teos/build/",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "(gdb) teos daemon start",
      "type": "cppdbg",
      "request": "launch",
      "program": "/mnt/hgfs/Workspaces/EOS/Logos/teos/build/teos",
      "args": ["daemon", "start", "-V", "-b", "--resync-blockchain", "1"],
      "stopAtEntry": false,
      "cwd": "/mnt/hgfs/Workspaces/EOS/Logos/teos/build/",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}